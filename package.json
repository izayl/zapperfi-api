{
  "name": "zapperfi-api",
  "version": "0.0.3",
  "description": "Unofficial wrapper for the Zapperfi API",
  "repository": "git@github.com:izayl/zapperfi-api.git",
  "author": "izayl <izayl@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/izayl/zapperfi-api/issues"
  },
  "homepage": "https://github.com/izayl/zapperfi-api#readme",
  "main": "dist/zapperfi-api.umd.js",
  "module": "dist/zapperfi-api.es5.js",
  "typings": "dist/types/zapperfi-api.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run build",
    "postpublish": "pinst --enable",
    "release": "standard-version --no-verify",
    "test": "jest",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts"
  },
  "eslintConfig": {
    "extends": "@izayl/eslint-config-ts",
    "ignorePatterns": [
      "dist/**/*"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@izayl/eslint-config-ts": "0.4.2",
    "@rollup/plugin-typescript": "8.3.3",
    "@types/jest": "28.1.6",
    "@types/node": "18.0.6",
    "dotenv": "16.0.1",
    "eslint": "8.20.0",
    "husky": "8.0.1",
    "istanbul-badges-readme": "1.8.2",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "pinst": "2.1.6",
    "rollup": "2.77.0",
    "standard-version": "9.5.0",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --cache --fix",
    "*.js": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.27.2"
  }
}
